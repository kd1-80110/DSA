package com.sunbeam;

public class TestEmployee {
	
	
	    public static Employee linearSearchById(Employee[] employees, int empId) {
	        for (Employee employee : employees) {
	            if (employee.getEmpid() == empId) {
	                return employee;
	            }
	        }
	        return null;//Emp not found
	    }
	    
