package com.sunbeam;

public class Queue {
	private int arr[];
	private int rear;
	private int front;
	private final int size;
	
	public Queue(int size)
	
	public void push(int data) {
		rear++;
		arr[rear]=data;
	}
	public void pop() {
		front++;
	}
	public int peek() {
		return arr[front+1];
	}
	public boolean isFull() {
		return rear==size-1;
	}
	public boolean isEmpty()
	
	

}
