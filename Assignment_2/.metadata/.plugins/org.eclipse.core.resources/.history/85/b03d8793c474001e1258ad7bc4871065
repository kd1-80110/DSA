package com.sunbeam;

import java.util.Iterator;

public class DescInsertionSort {
	public static void descInsertionSort(int arr[],int N) {
		int count=0;
		for (int i = 1; i < N; i++) {
			int temp=arr[i];
			int j=i-1;
			while(j>=0 && arr[j]>temp) {
				count++
			}
			
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
